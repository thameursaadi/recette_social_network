          description: $scope.pictureDescription,
          url: $scope.pictureUrl
        }
      }));
      return req.success(function(res) {
        if (res.success) {
          $scope.pictureDescription = "";
          $scope.pictureDescription = "";
          return $scope.pictureUrl = "";
        } else {
