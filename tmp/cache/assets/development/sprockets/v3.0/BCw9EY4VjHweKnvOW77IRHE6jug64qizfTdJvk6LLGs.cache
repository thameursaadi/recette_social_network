        params: {
          title: "$scope.recipeName",
          description: $scope.recipeDescription
        }
      });
      return req.success(function(res) {
        if (res.success === true) {
          this.cooking.value("recipeId", res.id);
          return $location.path("/addPictures");
        } else {
          return alert("error");
        }
      });
    };
